package com.cisco.apas.dao;

public class DAOSystemException extends DAOException{

	private static final long serialVersionUID = 1L;

	/**
	 * @param msg
	 * @param th
	 */
	public DAOSystemException(String msg, Throwable th) {
		super(msg, th);
	}

	/**
	 * @param th
	 */
	public DAOSystemException(Throwable th) {
		super(th);
	}

	DAOSystemException(){
		super();
	}
	
	DAOSystemException(String msg){
		super(msg);
	}
	
}
