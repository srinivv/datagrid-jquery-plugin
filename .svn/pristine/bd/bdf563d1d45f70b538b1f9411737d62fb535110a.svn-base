		this.qualRuleFor = qualRuleFor;
	}
	
	public void setQualRuleFor() {
		if(partnerType.contains("VAR") || partnerType.contains("DVAR") || partnerType.contains("SP")) {
			this.qualRuleFor = "P";	
		} else {
			this.qualRuleFor = "D";
		}
	}

