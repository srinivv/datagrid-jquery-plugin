<script type="text/template" id="applicableCountries">
	<div class="popupTitle">Select Country for Applicable</div>
		
	<div class="SelectALL">
		<input type="radio" value="P" name="applyFilterForApplicable[]" data-parentclass="applicablePop" data-value="EEA"> EEA
 		<input type="radio" value="P" name="applyFilterForApplicable[]" data-parentclass="applicablePop" data-value="NON_EEA"> NON EEA    
		<input type="radio" value="P" name="applyFilterForApplicable[]" data-parentclass="applicablePop" data-value="ALL">Select All
	</div>  
	<div class="clear"></div>
	<div>
		<@ _.each(countryData, function(countryRow) { 	@>	
			<div class="single-column">
			<h6 class="country_title"><@=countryRow.name@>
			<div class="SelectALL"><input type="checkbox" class="selectContLevel" data-parentclass="applicablePop">Select All</div>
			</h6>
			<div class="description_box">
				<div class="ckeckArea">
					<ul>
					<@ _.each(countryRow.countries, function(country) { @>
						<li><input type="checkbox" name="applicableCountries[]" value="<@= country.code @>"><@= country.name @></li>
					<@ });@>
					</ul>
				</div>
				<div class="clear"></div>
			</div>
		</div>
		<@ }); @>	
		<input type="button" class="fancyOK formButton" data-type="applicable" value="OK">
	</div>
</script>

