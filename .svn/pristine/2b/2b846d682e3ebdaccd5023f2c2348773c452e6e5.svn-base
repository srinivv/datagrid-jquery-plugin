<script type="text/template" id="get-rules-template">
<@ _.each(data, function(rule) { @>
	<li>
		<table width=100% border=0 cellspacing=0 cellpadding=0 class=countryListing>
			<tr class=deleteTR>
				<td width=30><input type=checkbox class=mycheckbox /></td>
                <td width=370><strong class=nodeName><@= rule.name @></strong> <span class=activeStatus>active</span><br clear=left/></td>
                <td width=150><@= rule.createdOn @><br/> <span class=createdBy><@= rule.createdOn @></span></td> 
                <td width=150><@= rule.updatedOn @><br/> <span class=createdBy><@= rule.updatedBy @></span></td>
                <td width=40>
    				<div class=posRel>
    					<div class=delRow>
    						<a title=Edit href=createViewRule.html>
    							<img class=paddingL3 src=images/edit-icon.png> </a>&nbsp;<a title=Delete href=#delicon class=delicon>
    							<img src=images/delRow.png align=delete>
							</a>
						</div>
					</div>
				</td>
			 </tr>
		</table>
	</li>
 
<@ }); @>
</script>

<script type="text/template" id="partnerType_accordion_template">
<ul>
<@ _.each(data, function(data) { @>
	<li><a href="javascript://" class="partner_li" data-name="<@=data.label @>" data-id="<@=data.id @>" data-value="<@=data.value @>" data-classification="<@=data.classification @>">
		<@=data.label @></a></li> 
<@ }); @>
</ul>
</script>

<script type="text/template" id="qualifications_accordion_template">
						<div class="subChild">
							<span class="subchildTitle certifIcon certifIconselect">Certification</span>
							<div class="listValues certivalue" style="display: block;">
								<ul>
									<@ _.each(data, function(data) {
										var classification = data.classification;
										switch(classification)  {
											case "Certification":  @>		
													<li><a href="javascript://" class="c1 certif_li cert" data-name="<@=data.label @>" data-id="<@=data.id @>" data-value="<@=data.value @>" 
														data-classification="<@=data.classification @>">
													<@=data.label @></a></li> 
 										<@ }@>	
									<@ }); @>									
								</ul>
							</div>
						</div>

						<div class="subChild">
							<span class="subchildTitle specIcon">Specialization</span>
							<div class="listValues specialvalues">
								<ul class="leftSubMenuScroll">
									<@ _.each(data, function(data) {
										var classification = data.classification;
										switch(classification)  {
											case "Specialization":  @>		
													<li><a href="javascript://" class="certif_li spec" data-name="<@=data.label @>" data-id="<@=data.id @>" data-value="<@=data.value @>" 
														data-classification="<@=data.classification @>">
													<@=data.label @></a></li> 
 										<@ }@>	
									<@ }); @>									
								</ul>
							</div>
						</div>

					<div class="subChild">
							<span class="subchildTitle authrIcon">Authorization</span>
							<div class="listValues authvalue">
								<ul class="leftSubMenuScroll">
									<@ _.each(data, function(data) {
										var classification = data.classification;
										switch(classification)  {
											case "Authorization":  @>		
													<li><a href="javascript://" class="certif_li auth" data-name="<@=data.label @>" data-id="<@=data.id @>" data-value="<@=data.value @>" 
														data-classification="<@=data.classification @>">
													<@=data.label @></a></li> 
 										<@ }@>	
									<@ }); @>									
								</ul>
							</div>
						</div>
</script>


