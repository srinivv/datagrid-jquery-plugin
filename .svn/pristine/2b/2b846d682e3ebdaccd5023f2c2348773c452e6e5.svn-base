<script type="text/template" id="ruleTopBlock">
<div class="preNodeArea">
		<div class="preNodebox1">
			<label>Node Name</label> <input type="text" name="nodeName" value="<%= request.getParameter("nodeName") %>">
		</div>
		<div class="preNodebox2">
			<label>Rule Name <span class="required tooltip" original-title="Mandatory">*</span></label> <input type="text" name="ruleName"  >
			
		</div>
		<div class="clear"></div>
	</div>

<div class="preCondition">
		<h6>Pre Conditions</h6>
		<div class="preConditionBox1">
			<big>Intended Use <span class="required tooltip" original-title="Mandatory">*</span></big> 
				<span class="checkboxoption"><input name="intendedUse[]" value="Resale"    type="checkbox">Resale</span>
				<span class="checkboxoption"><input name="intendedUse[]" value="Internal Business Use"    type="checkbox">Internal Business Use</span>
				<span class="checkboxoption"><input name="intendedUse[]" value="Lab"    type="checkbox">Lab</span>
				<span class="checkboxoption"><input name="intendedUse[]" value="Service Provision Use"    type="checkbox">Service Provision Use</span>
				<span class="checkboxoption"><input name="intendedUse[]" value="Managed Service"    type="checkbox">Managed Service</span>
				<span class="checkboxoption"><input name="intendedUse[]" value="Stocking"    type="checkbox">Stocking</span>
				<span class="checkboxoption"><input name="intendedUse[]" value="Donations"    type="checkbox">Donations</span>
		</div>
		<div class="preConditionBox2">
			<big>Select Countries <span class="required tooltip" original-title="Mandatory">*</span></big>
				 <a href="#applicablePop"	class="appCountries">Applicable Countries</a>	
				<a href="#billingPop"	class="billingCountries">Billing Countries</a>				 
				<a href="#installPop"	class="installCountries">Install Countries</a>	
				<a href="#errorPop"	class="errorPop" style="display:none"></a>	
				<a href="#genericMessage"	class="genericMessage" style="display:none"></a>			
		</div>
		<div class="preConditionBox1" style="padding-left:10px">
			<big>Apply Rule for <span class="required tooltip" original-title="Mandatory">*</span></big>  
				<span class="checkboxoption"><input type="radio"	name="applyRuleFor[]" value="P"> 1T Partner buying from Cisco </span>
				<span class="checkboxoption"><input type="radio" name="applyRuleFor[]" value="PD"> 1T/ 2T Partner buying from distributor</span>
				<span class="checkboxoption"><input type="radio" name="applyRuleFor[]" value="D">Distributor buying for himself from Cisco</span>
				<span class="checkboxoption"><input type="radio" name="applyRuleFor[]" value="DC"> Direct end customer buying from Cisco</span>				
		</div>
		<div class="preConditionBox4">
			<big>Effective Date <span class="required tooltip" original-title="Mandatory">*</span></big>[MM/DD/YYYY]
					<input type="text" class="input apasDatePick" name="startDate" id="startDate" value=""> 
					<input type="text" class="input apasDatePick" name="endDate"  id="endDate" value="">
		</div>
		<div class="clear"></div>
	</div>
</script>
