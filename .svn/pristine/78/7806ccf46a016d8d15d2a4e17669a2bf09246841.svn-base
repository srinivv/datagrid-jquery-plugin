package com.cisco.apas.util;

public class StringUtils {
	
	public static boolean isNumeric(String input) {
		try {
			Integer.parseInt(input);
			return true;
		} catch (Exception e) {
			return false;
		}
	}
	
	public static boolean isEmpty(String input) {
		if (input == null || "".equals(input.trim())) {
			return true;
		} 
		return false;
	}
}
