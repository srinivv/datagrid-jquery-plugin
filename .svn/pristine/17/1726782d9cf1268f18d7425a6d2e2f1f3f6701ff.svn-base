# DEFAULT CACHE REGION   

jcs.default=
jcs.default.cacheattributes=org.apache.jcs.engine.CompositeCacheAttributes
jcs.default.cacheattributes.MaxObjects=1000
jcs.default.cacheattributes.MemoryCacheName=org.apache.jcs.engine.memory.lru.LRUMemoryCache
jcs.default.cacheattributes.UseMemoryShrinker=true
jcs.default.cacheattributes.MaxMemoryIdleTimeSeconds=3600
jcs.default.cacheattributes.ShrinkerIntervalSeconds=60
jcs.default.cacheattributes.MaxSpoolPerRun=500
jcs.default.elementattributes=org.apache.jcs.engine.ElementAttributes
jcs.default.elementattributes.IsEternal=false

# lookup region
jcs.region.lookup=
jcs.region.lookup.cacheattributes=org.apache.jcs.engine.CompositeCacheAttributes
jcs.region.lookup.cacheattributes.MaxObjects=100
jcs.region.lookup.cacheattributes.MemoryCacheName=org.apache.jcs.engine.memory.lru.LRUMemoryCache
jcs.region.lookup.cacheattributes.UseMemoryShrinker=true
jcs.region.lookup.cacheattributes.MaxMemoryIdleTimeSeconds=63072000
jcs.region.lookup.cacheattributes.ShrinkerIntervalSeconds=600
jcs.region.lookup.cacheattributes.MaxSpoolPerRun=500
jcs.region.lookup.elementattributes=org.apache.jcs.engine.ElementAttributes
jcs.region.lookup.elementattributes.IsEternal=true

    
    