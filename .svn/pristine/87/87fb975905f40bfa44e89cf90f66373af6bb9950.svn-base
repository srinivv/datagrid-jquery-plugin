}

/* Auto complete loading */
.loading { 
	background:url(../images/indicator.gif) no-repeat right center #FFFFFF !important
}

input.spinner { 
	background-image: url("../images/inputBG.jpg"), url(../images/indicator.gif);
	background-repeat: repeat-x, no-repeat; 
	background-position: left -1px, right center;
}

.label_check input, .label_radio input {
