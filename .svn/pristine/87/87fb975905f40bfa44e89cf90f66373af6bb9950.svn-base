/*overlay css*/
.overlay {
	bottom: 0;
	left: 0;
	overflow-x: auto;
	overflow-y: scroll;
	position: fixed;
	right: 0;
	top: 0;
	z-index: 9999;
	background:url(../images/overlay_bg.png) repeat 0px 0px;
	display:none;
}
.popUpWindow {
