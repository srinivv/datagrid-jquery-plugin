/*Reset default browser CSS.-------------------------------------------------------------- */
html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, code, del, dfn, em, img, q, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, caption, tbody, tfoot, thead {
	margin: 0;
	padding: 0;
	border: 0;
	font-weight: inherit;
	font-style: inherit;
	font-size: 100%;
}
table {
	margin:0px;
	padding:0px;
	border-collapse:collapse;
}
tr, th, td {
	margin:0px;
	padding:0px;
}
/* Headings=====================================================*/
h1, h2, h3, h4, h5, h6 {
	line-height:normal;
	color:#666666;
    font-weight: normal;
    padding-bottom: 5px;
}
h1, h2, h3 {
	font-weight:bold;
}
h1 {
	font-size:30px;
}
h2 {
	padding-bottom:5px;
    color: #1E76C7;
    display: inline-block;
    font-size: 25px;
    font-weight: normal;
    margin: 0;
    padding: 0;
    text-transform: none;	
}
h3 {
	font-size:20px;
}
h4 {
	font-size:14px;
}
h6 {
	font-size:16px;
}
/* Lists===================================================== */
ul, ol, ul li {
	padding:0px;
	margin:0px;
}
li ul, li ol {
	padding:0px;
	margin:0px;
}
ul {
	list-style-type:none;
}
ol {
	list-style-type: decimal;
}
dl {
	padding:0px;
	margin:0px;
}
dl dt {
	font-weight: bold;
}
dd {
	padding:0px;
	margin:0px;
}
/* Remove annoying border on linked images. */
a img {
	border: none;
	outline:none;
}
a {
	outline:none;
	text-decoration:none;
	color:#185874;
}
input, textarea, select, a, img {
	outline:none;
}
a:hover {
	text-decoration:underline;
	color:#1a7caa;
}
.none {
	display:none;
}
input[type="text"], textarea, select {
	background-color: #FFFFFF;
	background-image: url(../images/inputBG.jpg);
	background-position: left -1px;
	background-repeat: repeat-x;
	border-color: #979797 #D2D2D2 #F4F4F4 #D5D5D5;
	border-radius: 3px 3px 3px 3px;
	border-style: solid;
	border-width: 0 1px 1px;
	color: #999999;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-weight: bold;
	outline: medium none;
	padding: 7px;
}
input.datepicker {
	background: url(../images/dateInput.png) no-repeat scroll right top #FFFFFF !important;
}
input.apasDatePick {
	background: url(../images/dateInput.png) no-repeat scroll right top #FFFFFF !important;
}
input[type=submit] {
	cursor:pointer;
}
input[type="checkbox"] {
	vertical-align:middle;
	margin-top:0px;
}
.disableBTN {
	background: url(../images/ButtonBG-disable.jpg) left top #CACACA;
	border: 1px solid #B4B4B4;
	border-radius: 3px 3px 3px 3px;
	color: #FFFFFF;
	cursor: auto;
	font-size: 13px;
	font-weight: bold;
	padding: 6px 8px;
	cursor:pointer;
	text-decoration:none;
}
.disableBTN:hover {
	color:#fff;
}
.error {
	color:#ff0000;
	font-size:11px;
}
/*common class*/
.clear {
	clear:both;
}
.clearnone {
	overflow: hidden;
	height:0px;
}
.alignleft {
	float:left;
}
.alignright {
	float:right;
}
.alignmiddle {
	text-align:center;
}
.textright {
	text-align:right;
}
.textleft {
	text-align:left;
}
/* Global CSS ==============================================*/
