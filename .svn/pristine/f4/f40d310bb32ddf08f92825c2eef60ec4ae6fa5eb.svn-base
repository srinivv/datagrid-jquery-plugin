<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="apasDataSource" class="org.springframework.jndi.JndiObjectFactoryBean">
                <property name="jndiName"><value>jdbc/APA_STAGE</value></property>
	</bean>
  	<bean id="nativeJdbcExtractor" class="org.springframework.jdbc.support.nativejdbc.WebSphereNativeJdbcExtractor" abstract="false" singleton="true" lazy-init="true" autowire="default" dependency-check="default" />	
</beans>
