		<tab id="partner" labelKey="label.tabs.partnerException" forward="fwdPartnerException" url="exception.do?actionType=create"/>
		<tab id="notifications" labelKey="label.tabs.notifications" forward="fwdNotifications" url="notification.do?actionType=show"/>
		<tab id="serviceXML" labelKey="label.tabs.serviceXMLUI" forward="fwdServiceXMLUI" url="service.do?actionType=view"/>
