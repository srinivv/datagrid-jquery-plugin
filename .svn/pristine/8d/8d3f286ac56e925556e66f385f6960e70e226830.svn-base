<?xml version="1.0" encoding="utf-8" ?>
	<xsd:schema xmlns:cisco="urn:cisco:names:message:sds" 
				xmlns:soapenc="http://schemas.xmlsoap.org/soap/encoding/" 
				xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
				xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
				xmlns:oa="http://www.openapplications.org/oagis/9" 
				xmlns:qdt="http://www.openapplications.org/oagis/9/qualifieddatatypes/1.2" 
				xmlns:cixs="http://www.cisco.com/cixs2.0/oagis" 
				targetNamespace="urn:cisco:names:message:sds" 
				elementFormDefault="qualified" version="1.3">
	<xsd:import namespace="http://www.openapplications.org/oagis/9" schemaLocation="OAGIS9-0-Components.xsd"/>
 <xsd:element name="ProcessAPSRequest" type="cisco:ProcessAPSRequestType" />
  <xsd:complexType name="ProcessAPSRequestType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="oa:BusinessObjectDocumentType">
        <xsd:sequence>
          <xsd:element name="DataArea" type="cisco:ProcessAPSRequestDataAreaType" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ProcessAPSRequestDataAreaType">
    <xsd:sequence>
      <xsd:element ref="oa:Process" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="APSRequest" type="cisco:APSType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="AcknowledgeAPSResponse" type="cisco:AcknowledgeAPSResponseType" />
  <xsd:complexType name="AcknowledgeAPSResponseType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="oa:BusinessObjectDocumentType">
        <xsd:sequence>
          <xsd:element name="DataArea" type="cisco:AcknowledgeAPSResponseDataAreaType" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AcknowledgeAPSResponseDataAreaType">
    <xsd:sequence>
      <xsd:element ref="oa:Acknowledge" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="APSResponse" type="cisco:APSType" />
    </xsd:sequence>
  </xsd:complexType>
 
 <xsd:complexType name="APSType">
       <xsd:sequence>
        <xsd:element minOccurs="0" name="RequestorApp" type="xsd:string" >
        <xsd:annotation>
	<xsd:documentation>Calling application short name like PDR, Order,Config.</xsd:documentation>
	</xsd:annotation>
       </xsd:element>

        <xsd:element minOccurs="0" name="RequestType" type="xsd:string">
        <xsd:annotation>
	<xsd:documentation>Not required</xsd:documentation>
	</xsd:annotation>
	</xsd:element>

        <xsd:element minOccurs="0" name="TransactionDate" type="oa:DateTimeType">
        <xsd:annotation>
	<xsd:documentation>Sysdate.</xsd:documentation>
	</xsd:annotation>
	</xsd:element>

        <xsd:element minOccurs="0" name="Status">
        <xsd:complexType>
	<xsd:annotation>
			<xsd:documentation>This is header level status sent back by APService to consumer it status will be populated  with following codes
			SUCCESS- when all the products are orderable.
			 ERROR - if there are products that does not qualify for Orderable
			.</xsd:documentation>
	</xsd:annotation>

        <xsd:sequence>
        <xsd:element name="Code" type="xsd:string" />
        <xsd:element minOccurs="0" name="Description" type="xsd:string" />
        </xsd:sequence>

        </xsd:complexType>
        </xsd:element>
    
	<xsd:element  minOccurs="0" name="IntendedUse" type="xsd:string" >
	</xsd:element>
    
        <xsd:element minOccurs="0" name="PartnerBEId" type="xsd:string" >
        <xsd:annotation>
        <xsd:documentation>Partner BEID  not begeo id or pdbid. </xsd:documentation>
        </xsd:annotation>
        </xsd:element>

      <xsd:element minOccurs="0" name="PartnerType" type="xsd:string" >
      <xsd:annotation>
          <xsd:documentation>
           This is returned for ATA the sample partner type are DVAR  etc..
          </xsd:documentation>
        </xsd:annotation>
       </xsd:element>

      <xsd:element minOccurs="0" maxOccurs="unbounded" name="PartnerAuthorization" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="PartnerSpecialization" type="xsd:string" />

      <xsd:element minOccurs="0" name="ApplicationSpecific" type="cisco:ApplicationSpecificType">
      <xsd:annotation>
          <xsd:documentation> Not in use </xsd:documentation>
        </xsd:annotation>
       </xsd:element>

      <xsd:element minOccurs="0" maxOccurs="unbounded" name="APSProduct" type="cisco:APSProductType" >
	<xsd:annotation>
          <xsd:documentation>
           This is the complex type for list of product to be validated by service.
          </xsd:documentation>
        </xsd:annotation>
	</xsd:element>
      </xsd:sequence>
      </xsd:complexType>
 
    <xsd:complexType name="ApplicationSpecificType">
    <xsd:annotation>
      <xsd:documentation source="http://www.cisco.com/cixs2.0/oagis">At times we have to communicate data across two specific applications such that we don't want to make any changes to the structure and the data we transfer does not fit into any standard cannonical element we are using </xsd:documentation>
      <xsd:documentation source="http://www.cisco.com/cixs2.0/oagis">Example : Update flags, Some propritory columns </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
    <xsd:element minOccurs="0" maxOccurs="unbounded" name="Data" type="oa:NameValuePairType" />
    </xsd:sequence>
   </xsd:complexType>

    <xsd:complexType name="APSProductType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="LineNumber" type="xsd:string">
      <xsd:annotation>
      <xsd:documentation>
      <CodeName>Not Requied.</CodeName>
      </xsd:documentation>
      </xsd:annotation>
      </xsd:element>

      <xsd:element minOccurs="0" maxOccurs="unbounded" name="PartnerAuthorization" type="xsd:string" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="PartnerSpecialization" type="xsd:string" />

      <xsd:element minOccurs="0" name="LineId" type="xsd:string" >
      <xsd:annotation>
       <xsd:documentation>
       <CodeName>Major Line id unique for each product this will be used to keep the track of SKU and major line ID for consumer like PDR.If not supplied it will not be returned..</CodeName>
       </xsd:documentation>
       </xsd:annotation>
       </xsd:element>

      <xsd:element minOccurs="0" name="LineType" type="xsd:string" />
      <xsd:element minOccurs="0" name="EndCustomerCountry" type="xsd:string" >
      <xsd:annotation>
          <xsd:documentation>
           ISO Country code for End customer
          </xsd:documentation>
        </xsd:annotation>
	</xsd:element>

      <xsd:element minOccurs="0" name="ParentLineId" type="xsd:string"/>
      <xsd:element minOccurs="0" name="RootParentLineId" type="xsd:string">
      <xsd:annotation>
          <xsd:documentation>
           For future extensibility
          </xsd:documentation>
        </xsd:annotation>
	</xsd:element>

      <xsd:element minOccurs="0" name="TechnologyGroup" type="xsd:string" >
	<xsd:annotation>
          <xsd:documentation>
		This is a response element that will be relayed to consumer.
          </xsd:documentation>
        </xsd:annotation>
     </xsd:element>

      <xsd:element minOccurs="0" name="SKU" type="xsd:string">
       <xsd:annotation>
          <xsd:documentation>
          Product Id from consumer
          </xsd:documentation>
        </xsd:annotation>
	</xsd:element>

      <xsd:element minOccurs="0" name="DistiFullfillment" type="xsd:string" >
	 <xsd:annotation>
          <xsd:documentation>
          This will be true when sku can be  disti fulfilled  and false otherwise.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>

      <xsd:element minOccurs="0" name="ApplicationSpecific" type="cisco:ApplicationSpecificType">
	 <xsd:annotation>
          <xsd:documentation>
          Future extensibiliyt owing to meet CICXS standards
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>

      <xsd:element minOccurs="0" name="Status">
        <xsd:complexType>
	 <xsd:annotation>
          <xsd:documentation>
          Code - This will be a predefined code for the error 
	  Description- The description for the error code.
          </xsd:documentation>
        </xsd:annotation>

        <xsd:sequence>
        <xsd:element name="Code" type="xsd:string" />
        <xsd:element minOccurs="0" name="Description" type="xsd:string" />
        </xsd:sequence>

        </xsd:complexType>
        </xsd:element>
        </xsd:sequence>
   </xsd:complexType>
</xsd:schema>

<!--
xjc -p  com.cisco.sd.service.jaxb.model .\APServiceRequest.xsd .\OAGIS9-0-Components.xsd .\UQDT.xsd -b .\sds.xjb
-->